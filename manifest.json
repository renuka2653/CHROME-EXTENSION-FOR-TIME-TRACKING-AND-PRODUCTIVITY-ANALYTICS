{
  "manifest_version": 3,
  "name": "Time Tracker & Productivity",
  "version": "1.0",
  "description": "Track time spent on websites and analyze productivity",
  "permissions": ["tabs", "storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icons/icon.png"
  }
}